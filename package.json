{
  "name": "react-twemoji-picker",
  "version": "0.1.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/types/index.d.ts",
  "repository": {
    "url": "https://github.com/brianhung/EmojiPicker"
  },
  "scripts": {
    "start": "vite",
    "build": "rm -r -f dist/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module es2020   --outDir dist/esm && copyfiles -u 1 src/**/*.css data/*.svg dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs && copyfiles -u 1 src/**/*.css data/*.svg dist/cjs",
    "build:website": "rm -r -f dist/ && vite build --sourcemap"
  },
  "dependencies": {
    "react-window": "^1.8.6",
    "react-window-infinite-loader": "^1.0.7"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "adm-zip": "^0.5.2",
    "copyfiles": "^2.4.1",
    "emoji-keywords": "github:brianhung/emoji-keywords",
    "gulp": "^4.0.2",
    "gulp-filter": "^6.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "lodash-es": "^4.17.20",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "superagent": "^6.1.0",
    "typescript": "^4.1.5",
    "vite": "^2.0.0-beta.70"
  }
}
